[run]
branch = True
omit =
    */tests/*
    */test_*.py
    # Generated protobuf files - auto-generated, not hand-written code
    mcpgateway/plugins/framework/external/grpc/proto/*_pb2*.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug
    raise AssertionError
    raise NotImplentedError
    if 0:
    if __name__ == .__main__.:

ignore_errors = True
